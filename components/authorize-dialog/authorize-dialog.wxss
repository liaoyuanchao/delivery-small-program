.wrap-dialog {
	line-height: 1.2;
}
.dialog-mask {
	position: fixed;
	top:0;
	right:0;
	bottom:0;
	left:0;
	background: rgba(0,0,0,.5);
	z-index: 1000;
}
.dialog-container {
	position: fixed;
	z-index: 10000;
	width:74%;
	max-width: 700rpx;
	top:50%;
	left:50%;
	-webkit-transform:translate(-50%,-50%);
	transform:translate(-50%,-50%);
	background-color: #fff;
	border-radius:6rpx;
	overflow: hidden;
	padding:30rpx;
}
.dialog-title {
	font-size: 32rpx;
}
.dialog-content {
	color:#494949;
}
.project-info {
	display: flex;
	align-items: center;
	margin:20rpx 0 30rpx;
}
.projectImage {
	width:90rpx;
	height: 90rpx;
	margin-right:18rpx;
}
.warn-image {
	width:60rpx;
	margin-right:10rpx;
}
.dialog-text {
	font-size: 26rpx;
	
}
.dialog-tips {
	
}
.tips-item {
	display: flex;
	align-items: center;
	font-size: 24rpx;
	color:#999;
}
.grey-dot {
	width:8rpx;
	height: 8rpx;
	background-color: #898989;
	border-radius:50%;
	margin-right:16rpx;
}
.dialog-footer {
	display: flex;
	align-items: center;
	justify-content: flex-end;
	margin-top:30rpx;
}
.dialog-btn {
	display: flex;
	align-items: center;
	justify-content: center;
	border:none;
	outline: none;
	overflow: hidden;
	background-color: transparent;
	box-shadow: 0px 0px 0rpx transparent;
	font-size: 24rpx;
	padding:20rpx 30rpx 10rpx;
}
.dialog-btn::after {
 border:0;
}
.grey-btn {
	color:#898989;
}
.red-btn {
	color:#fd8003;
}